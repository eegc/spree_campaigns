<% content_for :page_title do %>
  <%= plural_resource_name(Spree::Campaign) %>
<% end %>

<%= form_for [:admin, @object], html: { multipart: true } do |f| %>
  <div class="row">
    <div class="col-md-8">
      <div class="panel panel-default" data-hook="edit_dispatch_interval" >
        <div class="panel-body">
          <div class="col-md-12">
            <%= render partial: 'form', locals: { f: f } %>
          </div>

          <div class="form-actions" data-hook="buttons">
            <%= button Spree.t('actions.update'), 'save' %>
            <span class="or"><%= Spree.t(:or) %></span>
            <%= button_link_to Spree.t('actions.cancel'), spree.admin_dispatch_intervals_url, id: 'cancel_link', icon: 'delete' %>
          </div>
        </div>
      </div>
    </div>
  </div>
<% end %>
